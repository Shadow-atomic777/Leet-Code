class Solution {
public:
    int mySqrt(int x) {
        int l,h;
        l=1;
        h=x;
        
        while(l<=h){
          long long  mid=l+(h-l)/2;
          long long  sqr=mid*mid;
          if(x==sqr)
            return mid;
          else if(sqr<x)
            l=mid+1;
          else
            h= mid-1;
        }
        return h;
    }
};
